{
  "name": "attrcss",
  "description": "TailwindCSS using data attributes",
  "author": "Imed Adel <aadelimed@gmail.com>",
  "repository": "https://github.com/ImedAdel/attrcss.git",
  "version": "0.1.0",
  "main": "./src/index.js",
  "bin": {
    "attrcss": "./src/index.js",
    "attr": "./src/index.js"
  },
  "scripts": {
    "build:css": "node ./src/index.js build",
    "minify:css": "postcss attr.css > dist/attr.css",
    "prepare": "yarn build:css && yarn minify:css"
  },
  "files": [
    "src",
    "dist"
  ],
  "license": "MIT",
  "dependencies": {
    "deepmerge": "^4.2.2",
    "fs-extra": "^8.1.0",
    "lodash.kebabcase": "^4.1.1",
    "sade": "^1.7.3"
  },
  "keywords": [
    "tailwindcss",
    "attributes",
    "html",
    "data",
    "css",
    "framework"
  ],
  "devDependencies": {
    "cssnano": "^4.1.10",
    "husky": "^4.2.3",
    "postcss-cli": "^7.1.0",
    "prettier": "1.19.1",
    "pretty-quick": "^2.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
